# R8.10
# Aceptar
# Transformar un grafo en formato DOT de Graphviz.

START:
    CREATE DIGRAPH<INTEGER, EMPTY> g;

    INSERT INTO ceil INTEGER 17;
    FOR i FROM 1 TO ceil: # Arbol binario
        INSERT INTO g NODE<INTEGER> ("Node " + i);
        INSERT INTO parent INTEGER i/2;
        IF parent >= 1:
            INSERT INTO g EDGE<EMPTY> ("Node " + parent -> "Node " + i);

    DUMP g IN FILE "graph.out" IN DOT;

