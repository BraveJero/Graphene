# R8.5
# Aceptar
# Crear un grafo, iterar e imprimir el contenido en sus nodos.

START:
    CREATE DIGRAPH<INTEGER, EMPTY> g

    LET ceil BE INTEGER
    ceil <- 17
    FOR i FROM 1 TO ceil:
        INSERT INTO g NODE ("Node " + i, i)
        LET parent BE INTEGER
        parent <- i/2
        IF parent >= 1:
            INSERT INTO g EDGE ("Node " + parent -> "Node " + i)

    FOR n IN g WITH BFS ENTRY ("Node 1"):
        PRINT n.DATA
